{
  "name": "@leecheuk/wait-on",
  "description": "wait-on is a cross platform command line utility and Node.js API which will wait for files, ports, sockets, and http(s) resources to become available",
  "version": "8.0.3",
  "main": "lib/wait-on",
  "types": "index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "wait-on": "bin/wait-on"
  },
  "author": "Cheuk Lee <dev.cheuk.lee@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/leecheuk/wait-on.git"
  },
  "bugs": {
    "url": "http://github.com/leecheuk/wait-on/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint --fix \"lib/**/*.ts\" \"test/**/*.ts\"",
    "build": "rollup -c && chmod 755 ./bin/wait-on",
    "publish:next": "npm publish --tag next && npm view",
    "test": "vitest --config ./vitest.config.ts ./test"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "expect-legacy": "^1.20.2",
    "mkdirp": "^3.0.1",
    "mocha": "^10.2.0",
    "rollup": "^4.9.6",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "temp": "^0.9.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "joi": "^17.12.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "rxjs": "^7.8.1"
  },
  "keywords": [
    "wait",
    "delay",
    "cli",
    "files",
    "tcp",
    "ports",
    "sockets",
    "http",
    "exist",
    "ready",
    "available",
    "portable",
    "cross-platform",
    "unix",
    "linux",
    "windows",
    "win32",
    "osx"
  ]
}
